                "@version": 1.1,
                "@protected": true,
                "id": "@id",
                "type": "@type",
                "consumerNumber": {
                    "@id": "consumption:consumerNumber",
                    "@type": "xsd:string"
                },
                "fullName": {
                    "@id": "schema:name",
                    "@type": "xsd:string"
                },
                "premisesType": {
                    "@id": "consumption:premisesType",
