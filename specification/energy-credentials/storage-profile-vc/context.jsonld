{
    "@context": {
        "@version": 1.1,
        "@protected": true,
        "id": "@id",
        "type": "@type",
        "schema": "https://schema.org/",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "storage": "https://nfh-trust-labs.github.io/vc-schemas/energy-credentials/storage-profile-vc/context.jsonld#",

        "StorageProfileCredential": {
            "@id": "storage:StorageProfileCredential",
            "@context": {
                "@version": 1.1,
                "@protected": true,
                "id": "@id",
                "type": "@type",
                "consumerNumber": {
                    "@id": "storage:consumerNumber",
                    "@type": "xsd:string"
                },
                "fullName": {
                    "@id": "schema:name",
                    "@type": "xsd:string"
                },
                "meterNumber": {
                    "@id": "storage:meterNumber",
                    "@type": "xsd:string"
                },
                "assetId": {
                    "@id": "storage:assetId",
                    "@type": "xsd:string"
                },
                "storageCapacityKWh": {
                    "@id": "storage:storageCapacityKWh",
                    "@type": "xsd:decimal"
                },
                "powerRatingKW": {
                    "@id": "storage:powerRatingKW",
                    "@type": "xsd:decimal"
                },
                "commissioningDate": {
                    "@id": "storage:commissioningDate",
                    "@type": "xsd:date"
                },
                "storageType": {
                    "@id": "storage:storageType",
                    "@type": "xsd:string"
                }
            }
        },

        "UtilityIssuer": {
            "@id": "storage:UtilityIssuer",
            "@context": {
                "@version": 1.1,
                "@protected": true,
                "id": "@id",
                "type": "@type",
                "name": {
                    "@id": "schema:name",
                    "@type": "xsd:string"
                },
                "licenseNumber": {
                    "@id": "storage:licenseNumber",
                    "@type": "xsd:string"
                }
            }
        }
    }
}
